<div id="visDialog">
    <div style="max-height: 500px;overflow: scroll; min-width: 100%">
        <ul id="visList">
            <% list(columns, function(column) { %>
                <li>
                    <input type="checkbox" class="colVisCheckbox" <%= "id=\"vis-" + column.sTitle + "\" name=\"" + column.sTitle + "\" " + (column.bVisible? "checked=\"checked\"" : "")%>></input>
                    <label <%= "for=\"vis-" + column.sTitle + "\"" %>><%= column.sTitle %></label>
                </li>
            <% }); %>
        </ul>
    </div>
</div>
<div style="text-align:right">
    <button id="visDialogButton" class=".ui-button">Show / Hide Columns</button>
</div>